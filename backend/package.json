    "typescript": "^5.1.6",
    "typescript-eslint-parser": "^22.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.1",
    "rimraf": "^5.0.1"
  }
